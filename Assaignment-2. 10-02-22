#program for student result with six subjects
sname=input('enter student name')
sno=int(input('Enter student number'))
sgrp=input('Enter student group')
s1=int(input('enter JAVA marks'))
s2=int(input('enter OS marks'))
s3=int(input('enter AFM marks'))
s4=int(input('enter english marks'))
s5=int(input('enter hindi marks'))
s6=int(input('enter online business marks'))
total=s1+s2+s3+s4+s5+s6
avg=total/6
if s1>=35 and s2>=35 and s3>=35 and s4>=35 and s5>=35 and s6>=35:
    print("PASS",avg,total)
else:
    print("FAIL")
